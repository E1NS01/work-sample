{
    "name": "backend-test",
    "version": "1.0.0",
    "description": "Basic setup to for the backend engineer test",
    "main": "index.js",
    "scripts": {
        "start": "docker-compose up -d && NODE_ENV=production ts-node src/index.ts",
        "dev": "docker-compose up -d && NODE_ENV=development nodemon src/index.ts",
        "test": "set DEBUG=* && NODE_ENV=test jest --testPathPattern=test --coverage --detectOpenHandles"
    },
    "author": "Christoph Fey",
    "license": "ISC",
    "dependencies": {
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.1.0",
        "get-port": "^7.1.0",
        "mongoose": "^8.4.1",
        "winston": "^3.13.0"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.2",
        "@types/node": "20.13.0",
        "@types/supertest": "^6.0.2",
        "@typescript-eslint/eslint-plugin": "^5.60.1",
        "@typescript-eslint/parser": "^5.60.1",
        "eslint": "^8.43.0",
        "eslint-config-prettier": "^8.8.0",
        "eslint-plugin-prettier": "^4.2.1",
        "jest": "^29.5.0",
        "mongodb-memory-server": "^9.3.0",
        "nodemon": "^3.1.2",
        "prettier": "^2.8.8",
        "supertest": "^7.0.0",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.1.3"
    }
}
